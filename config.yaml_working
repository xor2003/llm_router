# config.yaml (в стиле документации LiteLLM)

# Общие настройки для прокси-сервера
proxy_server_config:
  port: 4000
  host: 0.0.0.0

# Список всех ваших моделей и ключей
model_list:
  # Даем всем моделям, которые должны работать в ротации, ОДНО И ТО ЖЕ ИМЯ.
  # Назовем его "my-router".
  - model_name: my-router
    litellm_params:
      model: gemini/gemini-2.5-pro-latest  # Official model name without prefixes
      api_key: os.environ/GEMINI_API_KEY
      stream: false
      rpm: 4 # Лимит для этой конкретной конфигурации все еще работает

  # Existing keys 1-3
  - model_name: my-router
    litellm_params:
      model: openrouter/moonshotai/kimi-k2:free
      api_key: os.environ/OPENROUTER_KEY1
      api_base: https://openrouter.ai/api/v1
      stream: false
      rpm: 8

